local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "MADE BY npc", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Teleporters",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Teleporters Section"
})

-- Add your teleporter buttons or functionality here within the Teleporters section

-- Example teleport button
Section:AddButton({
	Name = "Teleport to Spawn",
	Callback = function()
		-- Add your teleport code here
  		print("Teleporting to spawn")
  	end    
})

-- Example teleport list
local teleportLocations = {
	{ Name = "Teleport 1", Position = Vector3.new(10, 20, 30) },
	{ Name = "Teleport 2", Position = Vector3.new(-10, 15, 25) },
	-- Add more teleport locations as needed
}

for _, location in ipairs(teleportLocations) do
	Section:AddButton({
		Name = location.Name,
		Callback = function()
			-- Add your teleport code here using location.Position
	  		print("Teleporting to " .. location.Name)
	  	end    
	})
end

local Section = Tab:AddSection({
	Name = "Main Page"
})

local function flingAll()
    for _, v in pairs(workspace.SpawnedCars:GetChildren()) do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.PrimaryPart.CFrame
        task.wait(0.1)
    end
end

Section:AddButton({
	Name = "Fling All",
	Callback = function()
		flingAll()
  		print("button pressed")
  	end    
})

Section:AddButton({
	Name = "Fling GUI",
	Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/0Ben1/fe/main/obf_rf6iQURzu1fqrytcnLBAvW34C9N55kS9g9G3CKz086rC47M6632sEd4ZZYB0AYgV.lua.txt'), true))()
  		print("Fling GUI script loaded")
  	end    
})

Section:AddButton({
	Name = "Infinite Yield",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  		print("Infinite Yield script loaded")
  	end    
})

Section:AddButton({
	Name = "Open Any Door (client-sided)",
	Callback = function()
		game.Players.LocalPlayer.GroupInfo.Rank.Value = 255
  		print("Group rank set to 255")
  	end    
})

Section:AddButton({
	Name = "Scan for HR's!",
	Callback = function()
		local hrCount = countPlayersByRank(hrRankNames)
		sendNotification("HR SCAN", "There are " .. hrCount .. " HR's in-game.")
  		print("HR scan completed")
  	end    
})

Section:AddButton({
	Name = "Scan for MR's!",
	Callback = function()
		local mrCount = countPlayersByRank(mrRankNames)
		sendNotification("MR SCAN", "There are " .. mrCount .. " MR's in-game.")
  		print("MR scan completed")
  	end    
})

local isSpamming = false

Section:AddToggle({
	Name = "This is a toggle!",
	Default = false,
	Callback = function(Value)
		isSpamming = Value
		while isSpamming do
			game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(". g g / washieztroll", "All")
			wait(1)
		end
		print("Spam toggle:", Value)
	end    
})
