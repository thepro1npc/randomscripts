local groupId = 10261023
local rankNames = {"Junior Director",
    "Senior Director", "Head Director", "Corporate Intern", "Junior Corporate",
    "Senior Corporate", "Head Corporate", "Chief Human Resources Officer",
    "Chief Public Relations Officer", "Chief Operating Officer",
    "Chief Administrative Officer", "Developer", "Vice Chairman", "Chairman"
}

local function sendNotification(message, playerName)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "WARNING",
        Text = message .. " " .. playerName,
        Duration = 8
    })
end

local function handlePlayerEvent(player)
    local playerName = player.Name
    local success, rank = pcall(function()
        return player:GetRoleInGroup(groupId)
    end)

    if success then
        for _, rankName in ipairs(rankNames) do
            if rankName == rank then
                sendNotification("A " .. rankName .. " has joined the game. WATCH OUT!", playerName)
                break
            end
        end
    end
end

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Washiez fling all", "DarkTheme")
local Tab = Window:NewTab("Main Page")
local Section = Tab:NewSection("Main Page")

local function toggleFling()
    for _, v in pairs(workspace.SpawnedCars:GetChildren()) do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.PrimaryPart.CFrame
        task.wait(0.1)
    end
end

Section:NewButton("Toggle Fling", "ButtonInfo", function()
    toggleFling()
end)

Section:NewButton("Fling script", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/0Ben1/fe/main/obf_rf6iQURzu1fqrytcnLBAvW34C9N55kS9g9G3CKz086rC47M6632sEd4ZZYB0AYgV.lua.txt'),true))()
end)

Section:NewButton("Infinite Yield", "ButtonInfo", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

local TeleportersTab = Window:NewTab("Teleporters")
local TeleportersSection = TeleportersTab:NewSection("Teleporters")

TeleportersSection:NewButton("Teleport to Spawn", "ButtonInfo", function()
    -- Add your code to teleport to spawn here
end)

TeleportersSection:NewButton("Teleport to Home", "ButtonInfo", function()
    -- Add your code to teleport to home here
end)

game.Players.PlayerAdded:Connect(function(player)
    handlePlayerEvent(player)
end)

game.Players.PlayerRemoving:Connect(function(player)
    -- Optionally handle player removing event if needed
end)
